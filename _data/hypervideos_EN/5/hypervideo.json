            "frametrail:events": {},
            "frametrail:attributes": {
                "text": "&lt;p style=\"text-align: center;\"&gt;The Western Allies’ response &lt;br&gt;was to bring supplies to the city by air.&lt;br&gt;&lt;br&gt;The Berlin Airlift lasted from 24th June 1948 &lt;br&gt;until 12th May 1949. &lt;br&gt;&lt;/p&gt;&lt;br&gt;"
            }
        },
