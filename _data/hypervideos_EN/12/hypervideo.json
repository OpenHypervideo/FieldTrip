            "frametrail:events": {},
            "frametrail:attributes": {
                "text": "&lt;p style=\"text-align: center;\"&gt;Mansour A. has been living in the Tempohomes since 2018, waiting for his family.&lt;br&gt;&lt;/p&gt;&lt;br&gt;"
            }
        },
        {
            "@context": [
                "http://www.w3.org/ns/anno.jsonld",
                {
                    "frametrail": "http://frametrail.org/ns/"
                }
