            "frametrail:events": {},
            "frametrail:attributes": {
                "text": "&lt;p style=\"text-align: center;\"&gt;In 2017, temporary accommodation for 1000 refugees&lt;br&gt;was built on Tempelhof Field – the so-called Tempohomes.&lt;br&gt;&lt;/p&gt;&lt;br&gt;"
            }
        },
        {
            "@context": [
                "http://www.w3.org/ns/anno.jsonld",
                {
                    "frametrail": "http://frametrail.org/ns/"
                }
            ],
